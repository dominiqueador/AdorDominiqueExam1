        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="60dp"
        android:layout_height="56dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="204dp"
        android:layout_marginLeft="204dp"
        android:layout_marginTop="211dp"
        app:srcCompat="@drawable/fav"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="60dp"
        android:layout_height="56dp"
        android:layout_alignStart="@+id/app7"
        android:layout_alignTop="@+id/imageView5"
        app:srcCompat="@drawable/star"
        android:contentDescription="@string/todo" />

    <Button
        android:id="@+id/button4"
        android:layout_width="278dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/text3"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-81dp"
        android:layout_marginBottom="169dp"
        android:text="@string/button1" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="44dp"
        app:srcCompat="@drawable/alarm"
        android:contentDescription="@string/todo11" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/app"
        android:layout_alignTop="@+id/imageView8"
        app:srcCompat="@drawable/option"
        android:contentDescription="@string/todo10" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/imageView8"
        android:layout_alignEnd="@+id/text3"
        app:srcCompat="@drawable/info"
        android:contentDescription="@string/todo12" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="525dp"
        android:text="@string/t1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="163dp"
        android:layout_marginTop="525dp"
        android:text="@string/t2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="307dp"
        android:layout_marginTop="524dp"
        android:text="@string/t3" />

</RelativeLayout>